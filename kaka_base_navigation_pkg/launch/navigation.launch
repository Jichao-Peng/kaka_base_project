<launch>
	<!-- Run amcl -->
	<include file="$(find kaka_base_navigation_pkg)/launch/amcl.launch" /> 

	<!-- Run the map server with a blank map -->
	<!-- <node name="map_server" pkg="map_server" type="map_server" args="$(find kaka_base_navigation_pkg)/maps/blank_map.yaml"/>  -->

	<!-- Run move_base -->
	<include file="$(find kaka_base_navigation_pkg)/launch/move_base.launch" />

	<!-- Run a static transform between /odom and /map -->
	<!-- <node pkg="tf" type="static_transform_publisher" name="odom_map_broadcaster" args="0 0 0 0 0 0 /map /odom 100" />  -->


	<!-- Run a static transform between /laser and /base_link -->
	<node pkg="tf" type="static_transform_publisher" name="laser_baselink_broadcaster" args="0.335 0.36 0 -0.7853 0 0 /base_link /laser 100" />

</launch>
